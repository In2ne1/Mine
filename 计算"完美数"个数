#include<iostream>
using namespace std;
#include<vector>
//给定一个数，它的所有约数和(除自身外)等于该数，则称该数为完美数
//eg：28，约数为1，2，4，7，14，28，则1+2+4+7+14 = 28，28为完美数
//输入一个数字n，输出0到n内有多少个完美数(包括n)
class Solution
{
public:
	int perfectNum(int n)
	{
		int count = 0;
		int num = 0;
		vector<int> vv;
		while (n > 1)
		{
			for (int i = 1; i < n; i++)
			{
				if (n % i == 0)
				{
					vv.push_back(i);
				}
			}
			for (auto &ii : vv)
			{
				num += ii;
			}
			if (num == n)
			{
				count++;
			}
			num = 0;
			vv.clear();
			--n;
		}
		return count;
	}
};
int main()
{
	Solution aa;
	int n;
	cin >> n;
	cout << aa.perfectNum(n) << endl;
	
	return 0;
}
