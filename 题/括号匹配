#include<iostream>
#include<string>
#include<stack>
using namespace std;

bool IsValidExp(string s)
{
  stack<char> st;
  for (int i = 0; i < s.size(); i++)
  {
    if (s[i] == '(' || s[i] == '{' || s[i] == '[')
      st.push(s[i]);
    if (s[i] == ')')
    {
      if(st.empty())
        return false;
      if (st.top() == '(')
        st.pop();
      else
        return false;
    }
    if (s[i] == ']')
    {
      if(st.empty())
        return false;
      if (st.top() = '[')
        st.pop();
      else
        return false;
    }
    if (s[i] == '}')
    {
      if(st.empty())
        return false;
      if (st.top() = '{')
        st.pop();
      else
        return false;
    }
  }
  if (st.empty())
    return true;
  else
    return false;
}
int main()
{
  string s;
  cin >> s;
  cout << IsValidExp(s);
  return 0;

}
